<?xml version="1.0" encoding="utf-8"?>
<odoo>

     <record id="action_esthetic_test_view" model="ir.actions.act_window">
        <field name="name">Esthetic Test View</field>
        <field name="res_model">esthetic.quality.test</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>

    <record id="esthetic_quality_test_view_form" model="ir.ui.view">
        <field name="name">esthetic.quality.test.view.form</field>
        <field name="model">esthetic.quality.test</field>
        <field name="arch" type="xml">
            <form string=" Functional Test" edit="0" create="0">
                <sheet>
                    <group>
                        <group>
                            <field name="display_test"/>
                            <field name="case_test"/>
                        </group>
                        <group>
                            <field name="device_case"/>
                            <field name="device_charger"/>
                            <field name="cables"/>
                            <field name="headset"/>
                        </group>
                    </group>
                     <group>
                        <field name="create_date"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

</odoo>